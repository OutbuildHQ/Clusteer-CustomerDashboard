{
	"hosting": {
		"site": "clusboard",
		"source": ".",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"frameworksBackend": {
			"region": "europe-west1"
		},
		"rewrites": [
			{
				"source": "/assets/:asset/receive",
				"destination": "/assets/:asset/receive/index.html"
			},
			{
				"source": "/assets/:asset",
				"destination": "/assets/:asset/index.html"
			},
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	}
}
