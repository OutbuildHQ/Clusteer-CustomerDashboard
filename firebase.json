{
	"hosting": {
		"site": "clusboard",
		"public": "out",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"frameworksBackend": {
			"region": "europe-west1"
		},
		"rewrites": [
			{
				"source": "/assets/:asset",
				"destination": "/out/assets/:asset/index.html"
			}
		],
		"trailingSlash": true
	}
}
